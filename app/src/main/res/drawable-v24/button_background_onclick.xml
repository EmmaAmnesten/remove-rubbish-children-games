<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#2068767A"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item
        android:top="1dp"
        android:bottom="3dp"
        android:left="1dp"
        android:right="2dp">
        <shape>
            <gradient
                android:startColor="#F1E9A8"
                android:centerColor="#E1D99D"
                android:endColor="#BAB483"
                android:angle="360"
                android:centerY="65%"/>>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>